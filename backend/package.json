{
  "name": "crypto-news-agent-backend",
  "version": "1.0.0",
  "description": "Backend for crypto news AI agent",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node --transpile-only src/server.ts",
    "build": "tsc && npx prisma generate",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate"
  },
  "keywords": [
    "crypto",
    "news",
    "ai",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^1.1.0",
    "@langchain/langgraph": "^1.0.2",
    "@langchain/openai": "^1.1.3",
    "@langfuse/core": "^4.4.2",
    "@langfuse/langchain": "^4.4.2",
    "@langfuse/otel": "^4.4.2",
    "@langfuse/tracing": "^4.4.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@prisma/client": "^5.9.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "langchain": "^1.1.0",
    "langfuse": "^3.38.6",
    "node-cron": "^4.2.1",
    "openai": "^6.9.1",
    "prisma": "^5.9.1",
    "rss-parser": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/node-cron": "^3.0.11",
    "@vitest/ui": "^1.2.1",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}
